{
  "presets": [
    "env",
      "es2015",
    "react",
    "stage-0",
      "stage-3"
  ],
  "plugins": [
      "transform-decorators-legacy",
      "transform-class-properties",
      "react-hot-loader/babel",
    ["transform-runtime", {
      "helpers": false,
      "polyfill": false,
      "regenerator": true
    }],
      ["import", { "libraryName": "antd-mobile", "style": true}]
  ],
  "env": {
    "development": {
      "plugins": [
        "react-hot-loader/babel"
      ]
    }
  }
}
